<h1>New dataset_implementation</h1>

<% form_for(@dataset_implementation) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :site_id %><br />
    <%= f.text_field :site_id %>
  </p>
  <p>
    <%= f.label :dataset_id %><br />
    <%= f.text_field :dataset_id %>
  </p>
  <p>
    <%= f.label :start_date %><br />
    <%= f.date_select :start_date %>
  </p>
  <p>
    <%= f.label :end_date %><br />
    <%= f.date_select :end_date %>
  </p>
  <p>
    <%= f.label :update_frequency %><br />
    <%= f.text_field :update_frequency %>
  </p>
  <p>
    <%= f.label :indigenous_data %><br />
    <%= f.text_area :indigenous_data %>
  </p>
  <p>
    <%= f.label :user_notes %><br />
    <%= f.text_area :user_notes %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', dataset_implementations_path %>